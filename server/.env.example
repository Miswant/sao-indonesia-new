# Example env for server
# Port the server listens on
PORT=3001

# Rate limit window in hours (default 24)
RATE_LIMIT_HOURS=24

# Admin token to access /api/submissions
ADMIN_TOKEN=change_this_to_a_secure_value

# Static admin username/password (checked server-side). Defaults used if not provided:
# ADMIN_USERNAME=developer
# ADMIN_PASSWORD=P@ssword123!

# Lock duration after failed login attempts (minutes)
# ADMIN_LOCK_MINUTES=5

# Admin session TTL in minutes
# SESSION_TTL_MINUTES=30
